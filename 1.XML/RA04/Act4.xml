<!DOCTYPE persona [
	<!ELEMENT persona (nombre, mujer, fecha_de_nacimiento, ciudad)>
	<!ELEMENT nombre (#PCDATA)>
<!ELEMENT mujer EMPTY>
	<!ELEMENT fecha_de_nacimiento (dia #REQUIRED, mes #REQUIRED, anyo #FIXED)>
<!ATTLIST dia id #REQUIRED>
<!ATTLIST mes id #REQUIRED>
<!ATTLIST anyo #FIXED "2000">
<!ELEMENT ciudad (#PCDATA)>
]>

<persona>
    <nombre>Juan</nombre>
